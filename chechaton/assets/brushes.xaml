<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="TaskBarGreen" Color="#B0FF61"/>
    <SolidColorBrush x:Key="FailedOrange" Color="#FFB26A"/>
    <SolidColorBrush x:Key="WeekdayGreen" Color="#B8F956"/>
    <SolidColorBrush x:Key="WeekdayYellow" Color="#FFE458"/>
    <SolidColorBrush x:Key="HolidayGreen" Color="#B2FF3D"/>
    <SolidColorBrush x:Key="HolidayYellow" Color="#FFE142"/>
    <SolidColorBrush x:Key="NoMonthGrey" Color="#EBEAEA"/>
    <SolidColorBrush x:Key="ClickedGreen" Color="#84DA00"/>
    <SolidColorBrush x:Key="ClickedYellow" Color="#EAC400"/>
    <SolidColorBrush x:Key="ClickedGrey" Color="#D9D9D9"/>
    <SolidColorBrush x:Key="HoverGrey">#E0E0E0</SolidColorBrush>
    <SolidColorBrush x:Key="HoverGreen">#9AFF00</SolidColorBrush>
    <SolidColorBrush x:Key="HoverYellow">#FFD600</SolidColorBrush>
    <SolidColorBrush x:Key="HolidayYellowColor" >#FFE142</SolidColorBrush>

    <RadialGradientBrush x:Key="BackRadialGradient" 
                         GradientOrigin="0.5, 0.5" 
                         Center="0.5, 0.5" 
                         RadiusX="0.6" RadiusY="0.6">
        <GradientStop Color="#FFFFFF" Offset="0"/>
        <GradientStop Color="#F6FCFF" Offset="0.5"/>
        <GradientStop Color="#D1EFFF" Offset="1"/>
    </RadialGradientBrush>
    
</ResourceDictionary>